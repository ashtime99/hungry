<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ash.server.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ash.server.pojo.User">
        <id column="user_id" property="userId" />
        <result column="image_id" property="imageId" />
        <result column="user_username" property="userUsername" />
        <result column="user_password" property="userPassword" />
        <result column="user_age" property="userAge" />
        <result column="user_phone" property="userPhone" />
        <result column="user_nickname" property="userNickname" />
        <result column="user_locked" property="userLocked" />
        <result column="user_enabled" property="userEnabled" />
        <result column="user_login_time" property="userLoginTime" />
        <result column="user_create_time" property="userCreateTime" />
        <result column="user_update_time" property="userUpdateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, image_id, user_username, user_password, user_age, user_phone, user_nickname, user_locked, user_enabled, user_login_time, user_create_time, user_update_time
    </sql>

</mapper>
